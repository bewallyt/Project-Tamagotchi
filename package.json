{
  "name": "project-tamagotchi",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start-dev": "expo start --dev-client",
    "start-dev-clear": "expo start -c --dev-client",
