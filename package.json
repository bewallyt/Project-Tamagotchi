    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/three": "^0.147.1",
    "axios": "^1.2.1",
    "base64-arraybuffer": "^1.0.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "expo": "~47.0.12",
    "expo-dev-client": "~2.0.1",
    "expo-gl": "~12.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
