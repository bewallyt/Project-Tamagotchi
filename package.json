    "@rneui/themed": "^4.0.0-rc.7",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/three": "^0.147.1",
    "axios": "^1.2.1",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "expo": "~47.0.9",
    "expo-dev-client": "~2.0.1",
    "expo-gl": "~12.0.1",
    "expo-status-bar": "~1.4.2",
    "expo-three": "^7.0.0",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-animated-spinkit": "^1.5.2",
    "react-native-health": "^1.13.2",
